<settings>
    <boolean key="/apps/ControlPanel/CalendarFeed/EnableFeed" title="Publish to Feed"></boolean>
    <integer key="/apps/ControlPanel/CalendarFeed/FeedSize" title="Events Shown" min="1" max="5"></integer>
    <boolean key="/apps/ControlPanel/CalendarFeed/FillWithFuture" title="Fill with Future Events"></boolean>
    <boolean key="/apps/ControlPanel/CalendarFeed/LimitFuture" title="Limit Future Events"></boolean>
    <integer key="/apps/ControlPanel/CalendarFeed/LimitDaysSize" title="Limit Size (days)" min="1" max="31"></integer>
</settings>
